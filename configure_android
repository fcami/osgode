#!/bin/bash


cmake   -DOSGODE_USE_SINGLE_PRECISION=ON \
        -DOSGODE_BUILD_VERBOSE=OFF \
        -DOSGODE_BUILD_PLATFORM_ANDROID=ON \
        -DOSGODE_ODE_INCLUDE=$1/include \
        -DOSGODE_OSG_INCLUDE=$1/include \
        -DOSGODE_OSG_LIBRARIES=$1/lib \
        -DOSGODE_ODE_LIBRARIES=$1/lib \
        -DCMAKE_BUILD_TYPE=Release
